@charset "UTF-8";
/* 轮播图 */
.swiper swiper {
  width: 750rpx;
  height: 340rpx;
}

.swiper swiper image {
  width: 100%;
}

/* 导航 */
.nav {
  display: flex;
}

.nav navigator {
  flex: 1;
  padding: 20rpx;
}

.nav navigator image {
  width: 100%;
}

/* 楼层 */
.floor .floor_group .floor_title {
  padding: 10rpx 0;
}

.floor .floor_group .floor_title image {
  width: 100%;
}

.floor .floor_group .floor_list {
  overflow: hidden;
}

.floor .floor_group .floor_list navigator {
  width: 33.33%;
  float: left;
}

.floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  height: 27.72711vw;
  border-left: 10rpx solid #fff;
}

.floor .floor_group .floor_list navigator:nth-child(2), .floor .floor_group .floor_list navigator :nth-child(3) {
  border-bottom: 10rpx solid #fff;
}

.floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
